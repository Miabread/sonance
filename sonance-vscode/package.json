{
    "name": "sonance-vscode",
    "displayName": "sonance-vscode",
    "description": "",
    "version": "0.0.1",
    "publisher": "sonance",
    "engines": {
        "vscode": "^1.103.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "dependencies": {
        "vscode-languageclient": "^9.0.1"
    },
    "devDependencies": {
        "@types/node": "^25.2.0",
        "@types/vscode": "^1.108.1",
        "@vscode/test-electron": "^2.5.2"
    },
    "contributes": {
        "languages": [
            {
                "id": "sonance",
                "aliases": [
                    "Sonance",
                    "sonance"
                ],
                "extensions": [
                    ".son"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "sonance",
                "scopeName": "source.sonance",
                "path": "./syntaxes/sonance.tmLanguage.json"
            }
        ]
    }
}
